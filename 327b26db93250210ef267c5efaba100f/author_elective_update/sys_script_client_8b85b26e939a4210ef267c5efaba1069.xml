        <messages/>
        <name>Populate and Show Fine Paid Date</name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Get the returned date value
    var returnedDate = g_form.getValue('returned_date');

    // Check if returned date is not empty
    if (returnedDate) {
        // Set the fine paid date to the current date
        var currentDate = new GlideDateTime();
        g_form.setValue('fine_paid_date', currentDate.getDisplayValue());

        // Show the fine paid date field
        g_form.setDisplay('fine_paid_date', true);
    } else {
        // Hide the fine paid date field
        g_form.setDisplay('fine_paid_date', false);
    }
}]]></script>
