        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Update Fine Status to Paid</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    // Check if the field is changed and is not empty
    if (!isLoading && newValue !== oldValue) {
        // Check if the changed field is 'fine_paid_date'
        if (control == 'fine_paid_date') {
            var finePaidDate = g_form.getValue('fine_paid_date');

            if (finePaidDate) {
                // If the fine paid date is entered, set the Fine status to "fine paid"
                g_form.setValue('fine_status', 'fine paid');
            } else {
                // If the fine paid date is removed, set the Fine status to "not paid"
                g_form.setValue('fine_status', 'not paid');
            }
        }
    }
}]]></script>
