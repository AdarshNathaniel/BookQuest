        <condition/>
        <description>Informing the user that they can extend a book only once </description>
        <field>borrow_status</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Extend </name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    // Check if 'extend' option is selected
    if (newValue === 'extend') {
        // Show message below the field
        var message = 'You can extend this book only once.';
        g_form.showFieldMsg('borrow_status', message, 'info');
    }
}]]></script>
