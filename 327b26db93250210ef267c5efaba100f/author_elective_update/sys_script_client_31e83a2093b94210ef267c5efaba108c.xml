        <messages/>
        <name>Auto populate title</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {

    if (isLoading || newValue == '') {
        return;
    }

    var ga = new GlideAjax('x_1383151_bkquest.AutoPopulateTitle');
    ga.addParam('sysparm_name', 'gettitle');
    ga.addParam('sysparm_emp', newValue);
    ga.getXMLAnswer(setValue);

    function setValue(response) {
        var answer = response;
        if (answer) {
            var userDataObj = JSON.parse(answer);
            g_form.setValue('title', userDataObj.BookTitle);

        }
    }
}]]></script>
