        <script_true><![CDATA[function onCondition() {
    var bookId = g_form.getValue('book_id');

    if (bookId) {
        var ga = new GlideAjax('BookUtils');
        ga.addParam('sysparm_name', 'hasBeenExtended');
        ga.addParam('bookId', bookId);
        ga.getXMLAnswer(function(response) {
            var hasBeenExtended = response === 'true';
            updateBorrowStatusChoices(hasBeenExtended);
        });
    }

    function updateBorrowStatusChoices(hasBeenExtended) {
        if (hasBeenExtended) {
            g_form.removeOption('borrow_status', 'extend');
        } else {
            if (!g_form.getOption('borrow_status', 'extend')) {
                g_form.addOption('borrow_status', 'extend', 'Extend');
            }
        }
    }
}]]></script_true>
